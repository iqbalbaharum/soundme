module Collabeat declares *

data Beat:
  owner: string
  data_key: string
  cid: string

data Sheet:
  owner: string
  data_key: string
  cid: string
  beats: []Beat

service Collabeat:
  get_sheets(url: string, abi_url: string, start_block_in_hex: string, end_block_in_hex: string, address: string, topics: []string, ipfs_addr: string) -> []Sheet